/// The abstract interface for all the conv2d kernels generated by Aetherling
comp Conv2D<'G: II>(
    in[N]: ['G, 'G+H] 8,
) -> (
    out[N]: ['G+L, 'G+L+H] 8,
) with {
    /// The number of inputs the module accepts
    exists N where N > 0;
    /// The input hold time for the module
    exists H where H > 0;
    /// The latency of the module
    exists L where L > 0;
    /// The initiation interval of the module
    exists II where II > 0;
}