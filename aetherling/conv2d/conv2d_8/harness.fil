extern "conv2d_8.v" {
// KERNEL_START=Conv2d_8
comp Conv2d<G: 1>(
    clk: 1,
    @[G, G+1] I_0: 8,
    @[G, G+1] I_1: 8,
) -> (
    @[G+6, G+7] O_0: 8,
    @[G+6, G+7] O_1: 8,
);
// KERNEL_END=Conv2d_8
}

comp main<G: 1>(
    @interface[G] valid_up: 1,
    @[G, G+1] I_0: 8,
    @[G, G+1] I_1: 8,
) -> (
    @[G+6, G+7] O_0: 8,
    @[G+6, G+7] O_1: 8,
) {
    c := new Conv2d<G>(I_0, I_1);
    O_0 = c.O_0;
    O_1 = c.O_1;
}