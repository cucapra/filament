import "./conv2d.fil";

// The component needs to be run for an extra cycle to flush the last value
component main<G>(
    @interface[G, G+1] @phantom go: 1,
    @[G, G+1] in: 8,
) -> (
    @[G, G+1] out: 8,
) {
    s := new Stencil<G>(in);
    c := new Conv<G>(s.o1, s.o2, s.o3, s.o4, s.o5, s.o6, s.o7, s.o8, s.o9);
    out = c.out;
}