extern "map_200.v" {
    component Top<G>(
        clk: 1,
        reset: 1,
        @interface[G, G+1] valid_up: 1,
        @[G, G+1] I: 8,
    ) -> (
        @[G+4, G+5] valid_down: 1,
        @[G+4, G+5] O: 8
    );
}

component main<G>(
    @interface[G, G+1] valid_up: 1,
    @[G, G+1] in: 8
) -> (
    @[G+4, G+5] out: 8,
    @[G+4, G+5] valid_down: 1
) {
    m := new Top<G>(in);
    out = m.O;
    valid_down = m.valid_down;
}