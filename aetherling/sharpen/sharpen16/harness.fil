    comp Sharpen<G: 1>(
        clk: 1,
        @[G, G+1] I_0: 8,
    ) -> (
        @[G+8, G+9] O_0: 8,
        @[G+7, G+8] valid_down: 1,
    );
/* KERNEL_END=Sharpen_16 */
}

comp main<G: 1>(
    @interface[G] valid_up: 1,
