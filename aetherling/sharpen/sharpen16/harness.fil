extern "sharpen16.v" {
    component Sharpen<G>(
        clk: 1,
        @interface[G, G+1] @phantom valid_up: 1,
        @[G, G+1] I_0: 8,
    ) -> (
        @[G+8, G+9] O_0: 8,
        @[G+7, G+8] valid_down: 1,
    );
}

component main<G>(
    @interface[G, G+1] valid_up: 1,
    @[G, G+1] in: 8,
) -> (
    @[G+8, G+9] out: 8,
) {
    s := new Sharpen<G>(in);
    out = s.O_0;
}