import "../common/fast-interface.fil";
import "../common/stencil.fil";
import "../common/conv2d.fil";

comp Conv2dWrapper<'G:1>(
    I[N]: ['G, 'G+1] 8
) -> (
    O[N]: ['G+L, 'G+L+1] 8
) with {
    some N where N > 0, N <= 16, 16 % N == 0;
    some L where L > 0;
    let S = 8;
} {
    pc := new ParConv[S]<'G>(I{0..S});
    O{0..S} = pc.O{0..S};

    L := 6;
    N := S;
}