import "fp-mul.fil";

extern "fp-mult-nopipe.sv" {
  comp FP_Mult_NoPipe<G>(
      @[G, G+1] a: 32,
      @[G, G+1] b: 32,
  ) -> (
      @[G, G+1] exception: 1,
      @[G, G+1] overflow: 1,
      @[G, G+1] underflow: 1,
      @[G, G+1] res: 32,
  );
}

comp main<G>(
  @interface[G, G+1] _go: 1,
  @[G, G+1] left: 32,
  @[G, G+1] right: 32,
) -> (
  @[G, G+1] out: 32,
  @[G, G+1] verilog_nopipe: 32,
) {
  np := new FP_Mult_NoPipe<G>(left, right);
  f := new FPMult<G>(left, right);

  verilog_nopipe = np.res;
  out = f.out;
}
