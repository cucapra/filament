import "primitives/core.fil";
import "primitives/sequential.fil";

comp ALU<G: 2>(
    @[G, G+1] op: 1,
    @[G, G+1] left: 32,
    @[G, G+1] right: 32,
) -> (
    @[G, G+1] out: 32,
) {
    A := new Add[32];
    M := new Mult[32];
    a0 := A<G>(left, right);
    m0 := M<G>(left, right);
    mx := new Mux[32]<G>(op, a0.out, m0.out);
    out = mx.out;
}