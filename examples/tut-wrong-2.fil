import "primitives/core.fil";
import "primitives/sequential.fil";

comp ALU<G: 2>(
    @[G, G+1] op: 1,
    @[G, G+1] left: 32,
    @[G, G+1] right: 32,
) -> (
    @[G, G+1] out: 32,
) {
    A := new Add[32];
    M := new Mult[32];
    m0 := M<G>(left, right);
    a0 := A<G>(left, right);
    r0 := new Register[32]<G, G+3>(a0.out);
    mx := new Mux[32]<G+2>(op, r0.out, m0.out);
    out = mx.out;
}