extern "signed.sv" {
  component NegConst[WIDTH, VALUE]<G, ?L=G+1>(
   @interface[G, L] @phantom go: 1,
  ) -> (
    @[G, L] out: WIDTH,
  ) where L > G;

  component SAdd[WIDTH]<G, ?L=G+1>(
    @interface[G, L] @phantom go: 1,
    @[G, L] left: WIDTH,
    @[G, L] right: WIDTH
  ) -> (
    @[G, L] out: WIDTH,
  ) where L > G;
}