        @[G, G+1] left: #WIDTH,
        @[G, G+1] right: #WIDTH,
    ) -> (
        @[G, G+1] out: #WIDTH,
    );
    comp Div[#WIDTH]<G>(
        @[G, G+1] left: #WIDTH,
        @[G, G+1] right: #WIDTH,
    ) -> (
        @[G, G+1] out: #WIDTH,
    );
}