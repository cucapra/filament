generate(xls) using "xls.toml" {
    comp Butterfly[Exp, Mant]<'G:1>(
        clk: 1,
        in0_r: ['G, 'G+1] W,
        in0_i: ['G, 'G+1] W,
        in1_r: ['G, 'G+1] W,
        in1_i: ['G, 'G+1] W,
        twd_r: ['G, 'G+1] W,
        twd_i: ['G, 'G+1] W
    ) -> (
        out: ['G+L-1, 'G+L] 128
    ) with {
        let W = Exp+Mant+1;
        some L where L >= 1;
    };
}