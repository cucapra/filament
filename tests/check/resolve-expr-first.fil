comp Foo[#K]<G:1>() -> ( out: [G+#K, G+#K+1] 10) {}

// A Shift register that waits 2^#N cycles.
comp ShiftExp[#K]<G:1>(
     in: [G, G+1] 32
) -> (
     out: [G+#K, G+#K+1] 10
) {
    assume #K/2 + #K/2 == #K;
    foo := new Foo[#K/2]<G+#K/2>();
    out = foo.out;
}

comp main<G:1>() -> () {
    se := new ShiftExp[10]<G>(10);
}