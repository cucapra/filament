import "primitives/core.fil";

comp main<@G: 3>(
  go_G: interface[@G],
   a: [@G, @G+1] 32,
   b: [@G, @G+1] 32,
   c: [@G+2, @G+3] 32
) -> (
   out: [@G+2, @G+3] 32
) {

  A := new Add[32];
  M := new MultComb[32];
  R := new Register[32];

  m0 := M<@G>(a, b);
  r := R<@G, @G+3>(m0.out);
  a0 := A<@G+2>(r.out, c);

  out = a0.out;
}
