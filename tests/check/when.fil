import "primitives/core.fil";
import "primitives/sequential.fil";

component main<G>(
  @interface[G, G+3] go_G: 1,
  @[G, G+1] a: 32,
  @[G, G+1] b: 32,
  @[G+2, G+3] c: 32
) -> (
  @[G+2, G+3] out: 32
) {

  A := new Add[32];
  M := new MultComb[32];
  R := new Register[32];

  m0 := M<G>(a, b);
  r := R<G, G+3>(m0.out);
  a0 := A<G+2>(r.out, c);

  out = a0.out;
}
