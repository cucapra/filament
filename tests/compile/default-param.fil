import "primitives/core.fil";

comp main<G: 1>(
    en: interface[G],
     left: [G, G+1] 32,
     right: [G, G+1] 32,
) -> (
     out0: [G, G+1] 32,
     out1: [G, G+1] 64
) {
    m0 := new MultComb[32]<G>(left, right);
    m1 := new MultComb[32, 64]<G>(left, right);

    out0 = m0.out;
    out1 = m1.out;
}