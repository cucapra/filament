import "primitives/core.fil";

comp Let[#WIDTH]<G: 1>(
    @[G, G+1] in: #WIDTH,
) -> (
    @[G, G+1] out: #WIDTH+1,
) {
    let #OUT = #WIDTH+1;
    extended := new ZeroExtend[#WIDTH, #OUT]<G>(in);
    let ext: @[G, G+1] #OUT = extended.out;
    out = ext;
}

comp main<G: 1>(
    @[G, G+1] in: 32,
) -> (
    @[G, G+1] out: 33,
) {
    lp := new Let[32]<G>(in);
}