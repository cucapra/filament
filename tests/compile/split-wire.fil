import "primitives/comb.fil";

// Tests ConcatBundle by concatenating three 3-bit wires into a single 9-bit wire.
comp main<'G: 1>(
  in: ['G, 'G+1] 9,
) -> (
  out0: ['G, 'G+1] 3,
  out1: ['G, 'G+1] 3,
  out2: ['G, 'G+1] 3
) {
  concat := new SplitWire[3, 3]<'G>(in);

  out0 = concat.out{0};
  out1 = concat.out{1};
  out2 = concat.out{2};
}