comp Bundle<G: 3>(
    in[3]: for<#j> [G, G+3] 32,
) -> (
    out[3]: for<#j> [G+#j, G+#j+1] 32,
) {
    bundle a[6]: for<#k> [G, G+3] 32;
    a{3} = in{1};
    a{4} = a{3};
    a{5} = a{4};
    a{0} = a{5};
    a{1} = in{2};
    a{2} = in{0};
    out{0} = a{1};
    out{1} = a{2};
    out{2} = a{0};
}

comp main<G:3>(
   go: interface[G],
   ia: [G, G+3] 32,
   ib: [G, G+3] 32,
   ic: [G, G+3] 32,
) -> (
   a: [G,G+1] 32,
   b: [G+1,G+2] 32,
   c: [G+2,G+3] 32
) {
    bundle f[3]: for<#i> [G, G+3] 32;
    f{0} = ia;
    f{1} = ib;
    f{2} = ic;
    bun := new Bundle<G>(f{0..3});

    a = bun.out{0};
    b = bun.out{1};
    c = bun.out{2};
}