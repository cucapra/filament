extern component Mult<G>(
  @[G, G+1] left: 32,
  @[G, G+1] right: 32,
) -> (
  @[G+3, G+4] out: 32,
);

component main<G>() -> () {
  M := new Mult;

  // Immediately use the value generated by the adder in the multiplier.
  when G { m0 := M<G>(10, 20); }
  when G+3 { m1 := M<G+3>(m0.out, m0.out); }
}
