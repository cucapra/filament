import "primitives/state.fil";

comp main<'G: 4>(
  go: interface['G],
  in0: ['G, 'G+1] 32,
  in1: ['G+2, 'G+3] 32
) -> (
  out0: ['G+2, 'G+4] 32,
  out1: ['G+4, 'G+6] 32
) {
  Delay := new DelayRegister[32, 2, 2];

  delay1 := Delay<'G>(in0);
  delay2 := Delay<'G+2>(in1);

  out0 = delay1.out;
  out1 = delay2.out;
}